<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.munchado.orderprocess.ui.widgets.CustomTextView
        android:id="@+id/tv_print_setting"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginEnd="@dimen/fifteendp"
        android:layout_marginLeft="@dimen/fifteendp"
        android:layout_marginRight="@dimen/fifteendp"
        android:layout_marginStart="@dimen/fifteendp"
        android:layout_marginTop="60dp"
        android:gravity="center_vertical"
        android:text="Printer Settings"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/dark_grey"
        app:textStyle="AvenirNextCondensedMedium" />

    <com.munchado.orderprocess.ui.widgets.CustomTextView
        android:id="@+id/tv_profile_setting"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginEnd="@dimen/fifteendp"
        android:layout_marginLeft="@dimen/fifteendp"
        android:layout_marginRight="@dimen/fifteendp"
        android:layout_marginStart="@dimen/fifteendp"
        android:layout_marginTop="@dimen/fifteendp"
        android:gravity="center_vertical"
        android:text="Profile Settings"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/dark_grey"
        app:textStyle="AvenirNextCondensedMedium" />

    <com.munchado.orderprocess.ui.widgets.CustomTextView
        android:id="@+id/tv_order_processing_setting"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginEnd="@dimen/fifteendp"
        android:layout_marginLeft="@dimen/fifteendp"
        android:layout_marginRight="@dimen/fifteendp"
        android:layout_marginStart="@dimen/fifteendp"
        android:layout_marginTop="@dimen/fifteendp"
        android:gravity="center_vertical"
        android:text="Order Processing Settings"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/dark_grey"
        app:textStyle="AvenirNextCondensedMedium" />
</LinearLayout>
